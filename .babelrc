{
  "plugins": [
    [
      "module-resolver",
      {
        "cwd": "babelrc",
        "root": ["./src"],
        "extensions": [".js", ".ios.js", ".android.js"],
        "alias": {
          "_assets": "./src/assets",
          "_styles": "./src/assets/styles",
          "_images": "./src/assets/images",
          "_components": "./src/components",
          "_routes": "./src/routes",
          "_utils": "./src/utils",
          "_views": "./src/views",
          "_reducer": "./src/utils/redux/reducer"
        }
      }
    ]
  ]
}
